{
	"name": "tree-sitter-juice",
	"version": "0.0.1",
	"description": "juice grammar for tree-sitter",
	"main": "bindings/node",
	"keywords": [
		"parsing",
		"incremental"
	],
	"dependencies": {
		"nan": "^2.12.1"
	},
	"devDependencies": {
		"tree-sitter-cli": "^0.20.6"
	},
	"scripts": {
		"test": "tree-sitter test"
	},
	"tree-sitter": [
		{
			"scope": "juice",
			"file-types": [
				"juice"
			],
			"first-line-regex": "#!.*\\bjuice$"
		}
	]
}
